    "netlify-cli": "^17.23.8",
    "pkg": "^5.8.1",
    "postcss": "^8.4.38",
    "serverless-http": "^3.2.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vercel": "^34.2.3",
    "vite": "^5.2.0"
